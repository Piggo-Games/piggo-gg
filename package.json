{
  "name": "piggo-legends",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "web",
    "libs/contrib",
    "libs/core",
    "libs/games/playground"
  ],
  "scripts": {
    "dev": "npm run serve --workspace web",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "attack-time": "npm run workspace @piggo-legends/attack-time start",
    "docs": "npm run pages",
    "pages": "npm run build --workspace web && rm -r ./docs && mkdir docs && cp -r ./web/dist/* ./docs/"
  },
  "license": "ISC",
  "dependencies": {
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "html-loader": "^4.2.0",
    "http-server": "^14.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
